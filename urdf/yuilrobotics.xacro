  <joint name="Joint_1" type="revolute">
    <origin xyz="0.0 0.0 0.235" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="Link_1"/>
    <axis xyz="0.0 0.0 1.0"/>
    <limit upper="2.96706" lower="-2.96706" effort="100" velocity="100"/>
  </joint>

  <joint name="Joint_2" type="revolute">
    <origin xyz="0.16 -0.0769 0.235" rpy="0 0 0"/>
    <parent link="Link_1"/>
    <child link="Link_2"/>
    <axis xyz="-0.0 -1.0 0.0"/>
    <limit upper="1.570796" lower="-2.70526" effort="100" velocity="100"/>
  </joint>

  <joint name="Joint_3" type="revolute">
    <origin xyz="0.0 -0.0295 0.76" rpy="0 0 0"/>
    <parent link="Link_2"/>
    <child link="Link_3"/>
    <axis xyz="-0.0 -1.0 0.0"/>
    <limit upper="3.141593" lower="-1.396263" effort="100" velocity="100"/>
  </joint>

  <joint name="Joint_4" type="revolute">
    <origin xyz="0.3005 0.1064 0.2" rpy="0 0 0"/>
    <parent link="Link_3"/>
    <child link="Link_4"/>
    <axis xyz="1.0 0.0 0.0"/>
    <limit upper="6.283185" lower="0.0" effort="100" velocity="100"/>
  </joint>

  <joint name="Joint_5" type="revolute">
    <origin xyz="0.5165 -0.0685 0.0" rpy="0 0 0"/>
    <parent link="Link_4"/>
    <child link="Link_5"/>
    <axis xyz="0.0 -1.0 0.0"/>
    <limit upper="2.181662" lower="-2.181662" effort="100" velocity="100"/>
  </joint>

  <joint name="Joint_6" type="revolute">
    <origin xyz="0.063 0.0685 0.0" rpy="0 0 0"/>
    <parent link="Link_5"/>
    <child link="Link_6"/>
    <axis xyz="1.0 -0.0 -0.0"/>
    <limit upper="6.283185" lower="0.0" effort="100" velocity="100"/>
  </joint>

  <ros2_control name="yuilrobotics_hw" type="system">
    <hardware>
      <plugin>gazebo_ros2_control/GazeboSystem</plugin>
    </hardware>

    <joint name="Joint_1">
      <command_interface name="position"/>
      <state_interface name="position">
        <param name="initial_value">0.0</param>
      </state_interface>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>

    <joint name="Joint_2">
      <command_interface name="position"/>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>

    <joint name="Joint_3">
      <command_interface name="position"/>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>
    
    <joint name="Joint_4">
      <command_interface name="position"/>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>
    
    <joint name="Joint_5">
      <command_interface name="position"/>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>
    
    <joint name="Joint_6">
      <command_interface name="position"/>
      <state_interface name="position"/>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>    
  </ros2_control>

  <gazebo>
    <plugin filename="libgazebo_ros2_control.so" name="gazebo_ros2_control">
      <parameters>$(find yuilrobotics_description)/config/controller_manager.yaml</parameters>
    </plugin>
  </gazebo>

</robot>