    <collision>
      <origin xyz="-0.4605 -0.0 -1.43" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find yuilrobotics_description)/meshes/Link_4.stl" scale="0.001 0.001 0.001"/>
