    <visual>
      <origin xyz="-0.977 0.0685 -1.43" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find yuilrobotics_description)/meshes/Link_5.stl" scale="0.001 0.001 0.001"/>
