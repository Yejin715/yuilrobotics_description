  <link name="Link_3">
    <inertial>
      <origin xyz="0.05496043944024606 0.10419544747934753 0.058371524809330166" rpy="0 0 0"/>
      <mass value="64.002981497043"/>
      <inertia ixx="1.015493" iyy="1.327467" izz="1.115149" ixy="0.030653" iyz="0.035181" ixz="-0.469571"/>
