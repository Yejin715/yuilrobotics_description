      <origin xyz="0.26575828790948886 0.0012955340430784621 0.0009360053461771045" rpy="0 0 0"/>
      <mass value="47.53442539952465"/>
      <inertia ixx="0.245625" iyy="1.176996" izz="1.218782" ixy="0.001453" iyz="-0.000266" ixz="0.002266"/>
    </inertial>
    <visual>
      <origin xyz="-0.4605 -0.0 -1.43" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find yuilrobotics_description)/meshes/Link_4.stl" scale="0.001 0.001 0.001"/>
