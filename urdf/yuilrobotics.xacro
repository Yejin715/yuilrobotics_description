      <mass value="59.04937236680944"/>
      <inertia ixx="5.466906" iyy="5.531308" izz="0.365838" ixy="-0.035549" iyz="0.361063" ixz="0.083704"/>
    </inertial>
    <visual>
      <origin xyz="-0.16 0.0769 -0.47" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find yuilrobotics_description)/meshes/Link_2.stl" scale="0.001 0.001 0.001"/>
