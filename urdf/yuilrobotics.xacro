<?xml version="1.0" ?>
<robot name="yuilrobotics" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find yuilrobotics_description)/urdf/materials.xacro" />
  <xacro:include filename="$(find yuilrobotics_description)/urdf/yuilrobotics.trans" />
  <xacro:include filename="$(find yuilrobotics_description)/urdf/yuilrobotics.gazebo" />
 
  <link name="world"/>

  <joint name="base" type="fixed">
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <parent link="world"/>
      <child link="base_link"/>
      <axis xyz="0.0 0.0 0.0"/>
      <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
  </joint>

  <link name="base_link">
    <inertial>
      <origin xyz="-0.01714347087398209 0.0007777876351822993 0.1308887923264434" rpy="0 0 0"/>
      <mass value="97.64550480581015"/>
      <inertia ixx="1.428756" iyy="1.812942" izz="2.190694" ixy="0.019464" iyz="0.00385" ixz="-0.072787"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find yuilrobotics_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find yuilrobotics_description)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
    </collision>
  </link>

  <link name="Link_1">
    <inertial>
      <origin xyz="0.06317466133237605 0.035013511219838034 0.13725728484514843" rpy="0 0 0"/>
      <mass value="111.66153088402919"/>
      <inertia ixx="2.341514" iyy="2.318279" izz="2.327667" ixy="0.17489" iyz="-0.203995" ixz="-0.509045"/>
    </inertial>
    <visual>
      <origin xyz="-0.0 -0.0 -0.235" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find yuilrobotics_description)/meshes/Link_1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin xyz="-0.0 -0.0 -0.235" rpy="0 0 0"/>
